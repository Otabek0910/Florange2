@"
ENV=development
BOT_TOKEN=your_bot_token_here
YANDEX_GPT_API_KEY=your_yandex_api_key_here
DATABASE_URL=postgresql+asyncpg://user:pass@host:5432/dbname
REDIS_URL=redis://localhost:6379/0
WEBHOOK_URL=
FLORIST_CHANNEL_ID=
ARCHIVE_CHANNEL_ID=
AI_PROVIDER=yandex
"@ | Out-File -Encoding UTF8 .env

Add-Content .gitignore ".env.development"
Add-Content .gitignore "`n__pycache__/" 
Add-Content .gitignore "`n*.pyc"

git add .env .gitignore
git commit -m "Return .env as template; ignore local env and pyc"
